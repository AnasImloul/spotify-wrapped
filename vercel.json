{
  "buildCommand": "npm run validate && npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps --no-audit --no-fund",
  "framework": "vite",
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "master": true
    }
  }
}
